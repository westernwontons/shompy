datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "cargo prisma"
  output   = "../ui/src/prisma.rs"
}

model Food {
  id        Int      @id @default(autoincrement())
  food_type String   @db.VarChar(20)
  name      String   @db.VarChar(200)
  price     Float    @db.Real
  date      DateTime @db.Date
}
